  <data name="DefaultProcessWhitelist" xml:space="preserve">
    <value>// Un nome di processo per riga
// Doppio slash e contenuto dopo di esso in ogni riga verranno ignorati
// Supports "?" and "*" wildcards
    
// Non noi stessi
EnergyStar.exe
EnergyStarX.exe
// Edge ha consapevolezza energetica
msedge.exe
WebViewHost.exe
// Il frame UWP ha una gestione speciale, non dovrebbe essere limitato
ApplicationFrameHost.exe
// L'estintore non dovrebbe prendere fuoco
taskmgr.exe
procmon.exe
procmon64.exe
// Widget
Widgets.exe
// Shell di sistema
dwm.exe
explorer.exe
ShellExperienceHost.exe
StartMenuExperienceHost.exe
SearchHost.exe
sihost.exe
fontdrvhost.exe
// IME
ChsIME.exe
ctfmon.exe
// Servizi di sistema - hanno la loro consapevolezza
csrss.exe
smss.exe
svchost.exe
// WUDF
WUDFRd.exe

// Firefox 108 ora supporta la modalità di efficienza
firefox.exe</value>
